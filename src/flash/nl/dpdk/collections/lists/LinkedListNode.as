package nl.dpdk.collections.lists {/** * node class to hold the references to other nodes in the double linked list * and also holds the data item */internal class  LinkedListNode {	/**	 * the data item that can hold data of any type	 */	public var data : *;	/**	 * a reference to the next node in a double linked list	 */	public var next : LinkedListNode;	/**	 * a reference to the previous node in a double linked list	 */	public var previous : LinkedListNode;	public function LinkedListNode(data : * = null) {		this.data = data;	}		public function toString() : String {		return "Node: " + data;		}}}